    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/vanta@0.5.21/dist/vanta.fog.min.js">
    <link rel="stylesheet" href="/theFuture.css">


  </head>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/r123/three.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/vanta/0.5.21/vanta.fog.min.js"></script>

  <body id="vantajs" >
      <div class="centered">
        <div id="alterME2" class="glow">
          <div id="alterME" class="clip-animation";>
            <iframe id="player" width="500px" height="500px" src="https://www.youtube-nocookie.com/embed/<%=theLink%>?modestbranding=1&start=<%=begin%>&end=<%=end%>&autoplay=1&mute=1" frameborder="0" allow="autoplay" allowfullscreen"></iframe>
          </div>
        </div> 
      </div>
  
    
    <script src="index.js" async defer></script>

    <!-- THIS CODE IS HERE BECAUSE YOU CAN'T USE VARIABLE IN THE ON-PAGE STYLE USED ABOVE  -->
    <script>
      document.getElementById('alterME').style.setProperty("animation", "clip <%=animationLength %>s");
      document.getElementById('alterME2').style.setProperty("animation", "blink <%=animationLength %>s alternate");
    </script>
    
    <script>
      VANTA.FOG({
        el: "#vantajs",
        mouseControls: true,
        touchControls: true,
        gyroControls: false,
        minHeight: 200.00,
        minWidth: 200.00,
        highlightColor: 0xefff00,
        midtoneColor: 0xff00,
        lowlightColor: 0xffd4,
        baseColor: 0x50101,
        blurFactor: 0.23,
        // speed: 4.80
      })
    </script>




  </body>
